{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "defaultValue": "North Europe",
            "allowedValues": [ "North Europe", "UK South" ]
        },
        "storageAccountName": {
            "type": "string",
            "defaultValue": "sgdanmansw012"
        },
        "storageAccountType": {
            "type": "string",
            "defaultValue": "Standard_LRS"
        },
        "storageAccountKind": {
            "type": "string",
            "defaultValue": "StorageV2"
        },
        "profileName": {
            "type": "string",
            "defaultValue": "swprofile01"
        },
        "sku": {
            "type": "object",
            "defaultValue": {
                "name": "Standard_Microsoft"
            }
        },
        "dnszone_name": {
            "defaultValue": "mycloudproject.online",
            "type": "String"
        },
        "hostingPlanName": {
            "type": "string",
            "defaultValue": "danmanhostingplan"
        },
        "functionappname": {
            "type": "string",
            "defaultValue": "danfuncapp"
        }
    },
    "functions": [],
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Storage/storageAccounts",
            "apiVersion": "2022-05-01",
            "name": "[parameters('storageAccountName')]",
            "location": "[parameters('location')]",
            "kind": "[parameters('storageAccountKind')]",
            "sku": {
                "name": "[parameters('storageAccountType')]"
            },
            "properties": {
                "supportsHttpsTrafficOnly": true,
                "defaultToOAuthAuthentication": false
            }
        },
        {
            "type": "Microsoft.Network/dnszones",
            "apiVersion": "2018-05-01",
            "name": "[parameters('dnszone_name')]",
            "location": "global",
            "properties": {
                "zoneType": "Public"
            }
        },
        {
            "type": "Microsoft.Network/dnszones/CNAME",
            "apiVersion": "2018-05-01",
            "name": "[concat(parameters('dnszone_name'), '/resume')]",
            "dependsOn": [
                "[resourceId('Microsoft.Network/dnszones', parameters('dnszone_name'))]",
                "[resourceId('Microsoft.Storage/storageAccounts', parameters('storageAccountName'))]"
            ],
            "properties": {
                "TTL": 60,
                "CNAMERecord": {
                    "cname": "[concat(parameters('storageAccountName'), '.azureedge.net')]"
                },
                "targetResource": {}
            }
        },
        {
            "apiVersion": "2023-05-01",
            "name": "[parameters('profileName')]",
            "location": "[parameters('location')]",
            "sku": "[parameters('sku')]",
            "type": "microsoft.cdn/profiles"
        },
        {
            "apiVersion": "2023-05-01",
            "dependsOn": [
                "[concat('Microsoft.cdn/profiles/', parameters('profileName'))]",
                "[concat('Microsoft.Storage/storageAccounts/', parameters('storageAccountName'))]"
            ],
            "name": "[concat(parameters('profileName'),'/',parameters('storageAccountName'))]",
            "location": "[parameters('location')]",
            "type": "microsoft.cdn/profiles/endpoints",
            "properties": {
                "originHostHeader": "[concat(parameters('storageAccountName'), '.z16.web.core.windows.net')]",
                "contentTypesToCompress": [
                    "text/plain",
                    "text/html",
                    "text/css",
                    "application/x-javascript",
                    "text/javascript"
                ],
                "isHttpAllowed": true,
                "isHttpsAllowed": true,
                "queryStringCachingBehavior": "IgnoreQueryString",
                "origins": [
                    {
                        "name": "origin1",
                        "properties": {
                            "hostName": "[concat(parameters('storageAccountName'), '.z16.web.core.windows.net')]"
                        }
                    }
                ]
            }
        },
        {
            "type": "Microsoft.Cdn/profiles/endpoints/customDomains",
            "apiVersion": "2022-11-01-preview",
            "name": "[concat(parameters('profileName'),'/',parameters('storageAccountName'),'/','resume-mycloudproject-online')]",
            "dependsOn": [
                "[resourceId('Microsoft.Cdn/profiles/endpoints', parameters('profileName'), parameters('storageAccountName'))]",
                "[resourceId('Microsoft.Network/dnszones/CNAME', parameters('dnszone_name'), 'resume')]"

            ],
            "properties": {
                "hostName": "resume.mycloudproject.online"
            }
        },
        {
            "apiVersion": "2018-11-01",
            "name": "[parameters('functionappname')]",
            "type": "Microsoft.Web/sites",
            "kind": "functionapp",
            "location": "[parameters('location')]",
            "properties": {
                "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', parameters('hostingPlanName'))]",
                "siteConfig": {
                    "appSettings": [
                        {
                            "name": "FUNCTIONS_EXTENSION_VERSION",
                            "value": "~4"
                        },
                        {
                            "name": "FUNCTIONS_WORKER_RUNTIME",
                            "value": "node"
                        },
                        {
                            "name": "WEBSITE_NODE_DEFAULT_VERSION",
                            "value": "~18"
                        },
                        {
                            "name": "AzureWebJobsStorage",
                            "value": "[concat('DefaultEndpointsProtocol=https;AccountName=',parameters('storageAccountName'),';AccountKey=',listKeys(resourceId('b454ea40-6d37-4942-926a-a63094de3c14','web-grp','Microsoft.Storage/storageAccounts', parameters('storageAccountName')), '2019-06-01').keys[0].value,';EndpointSuffix=','core.windows.net')]"
                        },
                        {
                            "name": "WEBSITE_CONTENTAZUREFILECONNECTIONSTRING",
                            "value": "[concat('DefaultEndpointsProtocol=https;AccountName=',parameters('storageAccountName'),';AccountKey=',listKeys(resourceId('b454ea40-6d37-4942-926a-a63094de3c14','web-grp','Microsoft.Storage/storageAccounts', parameters('storageAccountName')), '2019-06-01').keys[0].value,';EndpointSuffix=','core.windows.net')]"
                        }
                    ],
                    "cors": {
                        "allowedOrigins": [
                            "https://portal.azure.com"
                        ]
                    }
                },
                "publicNetworkAccess": "Enabled",
                "httpsOnly": true
            },
            "dependsOn": [
                "[concat('Microsoft.Web/serverfarms/', parameters('hostingPlanName'))]",
                "[resourceId('Microsoft.Storage/storageAccounts', parameters('storageAccountName'))]"
            ]
        },
        {
            "apiVersion": "2020-12-01",
            "name": "[parameters('hostingPlanName')]",
            "type": "Microsoft.Web/serverfarms",
            "location": "[parameters('location')]",
            "sku": {
                "name": "Y1"
            },
            "properties": {}
        }
    ],
    "outputs": {}
}
